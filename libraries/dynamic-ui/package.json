{
  "name": "@dynamic-framework/ui",
  "version": "1.1.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "sass": "src/style/main.scss",
  "style": "dist/css/desgin-system.css",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/interface.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system/design-system.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modyo-dynamic/modyo-design-system.git"
  },
  "bugs": {
    "type": "git",
    "url": "https://github.com/modyo-dynamic/modyo-design-system.git"
  },
  "author": {
    "name": "Abraham José González Barboza",
    "email": "agonzalez@modyo.com",
    "url": "https://github.com/abe-modyo"
  },
  "files": [
    "dist/",
    "components/",
    "loader/",
    "src/style"
  ],
  "scripts": {
    "build": "pnpm build:stencil && pnpm build:style",
    "build:stencil": "stencil build --docs",
    "build:stencil:watch": "stencil build --docs --watch",
    "build:scss": "sass --load-path=node_modules --source-map --style=compressed src/style/main.scss dist/css/design-system.css",
    "build:scss:watch": "sass --load-path=node_modules --source-map -w src/style/main.scss dist/css/design-system.css",
    "build:autoprefixer": "postcss dist/css/design-system.css --use autoprefixer -o dist/css/design-system.css",
    "build:style": "pnpm build:scss && pnpm build:autoprefixer",
    "test": "stencil test --spec --e2e",
    "test:unit": "stencil test --spec",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "eslint": "eslint src/**/*{.ts,.tsx}",
    "stylelint": "stylelint \"**/*.{css,scss}\" --rd",
    "lint-staged": "lint-staged",
    "serve": "stencil build --dev --watch --serve"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@stencil/core": "^3.3.0",
    "@stencil/store": "^2.0.7",
    "bootstrap": "^5.3.0",
    "currency.js": "^2.0.4"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.7.1",
    "@stencil/eslint-plugin": "workspace:^0.4.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/vue-output-target": "^0.8.6",
    "@types/bootstrap": "^5.2.6",
    "@types/jest": "^27.0.3",
    "@types/luxon": "~3.0.1",
    "@types/node": "^18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.1.2",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.30.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lint-staged": "^12.4.1",
    "liquidjs": "^9.39.2",
    "luxon": "~3.0.3",
    "postcss-cli": "^10.1.0",
    "puppeteer": "^14.0.0",
    "sass": "^1.51.0",
    "stylelint": "^15.6.2",
    "stylelint-config-twbs-bootstrap": "^10.0.0",
    "typescript": "~4.4.4"
  },
  "peerDependencies": {
    "axios": "^1.1.2",
    "liquidjs": "^9.39.2",
    "luxon": "~3.0.3"
  },
  "license": "MIT"
}
